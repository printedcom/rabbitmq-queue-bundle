{
  "name": "printed/rabbitmq-queue-bundle",
  "description": "A helper wrapper for RabbitMQ producers and consumers",
  "type": "library",
  "keywords": ["queue", "rabbitmq", "symfony", "bundle"],
  "homepage": "https://printed.com",
  "license": "MIT",

  "authors": [
    {
      "name": "Printed Developers",
      "role": "Developer"
    }
  ],

  "autoload": {
    "psr-4": {
      "Printed\\Bundle\\Queue\\": "src/Printed/Bundle/Queue"
    }
  },

  "require": {
    "php": ">=8.1",

    "symfony/config": "^6.0",
    "symfony/console": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/expression-language": "^6.0",
    "symfony/filesystem": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/validator": "^6.0",

    "doctrine/cache": "^2.0",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/orm": "^2.14",

    "monolog/monolog": "^2.8",

    "php-amqplib/rabbitmq-bundle": "^2.11",

    "ramsey/uuid": "^4.7",
    "php-http/guzzle7-adapter": "^1.0",
    "andrewmy/rabbitmq-management-api": "^2.1",
    "psr/log": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
